@model Movie
@{
    Layout = "_Layout2";
}



        <h1>Filmler</h1>

        <div id="Filmler">
        
            <div class="film">
        
            <p>@Model.Title</p>
            <p>@Model.Description</p>
            <p>@Model.Director</p>
            <p>
                <ul>
                    @foreach (var item in @Model.Players)
                    {
                        <li>@item</li>
                    }
                </ul>

                
            </p>
        
            </div>

        </div>