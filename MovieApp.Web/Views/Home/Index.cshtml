@model MovieApp.Web.Models.Movie

<html>
       
<head>
        <title>

        </title>

</head>
    <body>
        <h1>Film Listesi</h1>

        <div id="Filmler">
        
            <div class="film">
        
            <p>@Model.Title</p>
            <p>@Model.Description</p>
            <p>@Model.Director</p>
            <p>
                <ul>
                    @foreach (var item in @Model.Players)
                    {
                        <li>@item</li>
                    }
                </ul>

                
            </p>
        
            </div>

        </div>


    </body>
</html>